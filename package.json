    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "mocha": "^2.4.5",
    "onchange": "^2.0.0"
  }
