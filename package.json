{
  "name": "node-es7-starter",
  "version": "0.0.1",
  "description": "Node starter based on ES7",
  "license": "MIT",
  "repository": "ButuzGOL/node-es7-starter",
  "author": {
    "name": "ButuzGOL",
    "email": "ButuzGOL@gmail.com",
    "url": "https://butuzgol.github.io"
  },
  "engines": {
    "node": ">=4.2.6"
  },
  "scripts": {
    "start": "babel-node src/index.js & npm run watch",
